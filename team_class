class Team:
    def __init__(self, name, fgm, fga, fg3a, fg3m, fta, ftm, oreb, dreb, ast, stl, tov, blk, pf, pts):
        self.name = name
        self.fgm = fgm
        self.fga = fga
        self.fg3a = fg3a
        self.fg3m = fg3m
        self.fta = fta
        self.ftm = ftm 
        self.oreb = oreb
        self.dreb = dreb
        self.ast = ast
        self.stl = stl
        self.tov = tov
        self.blk = blk
        self.pf = pf
        
