#converting each teams respective gamelog/data file to usable variables 

sixers_data = pd.read_csv('gamelog_sixers')
sixers_fgm = sixers_data.FGM.sum() / len(sixers_data)
sixers_fga = sixers_data.FGA.sum() / len(sixers_data)
sixers_fg3a = sixers_data.FG3A.sum() / len(sixers_data)
sixers_fg3m = sixers_data.FG3M.sum() / len(sixers_data)
sixers_fta = sixers_data.FTA.sum() / len(sixers_data)
sixers_ftm = sixers_data.FTM.sum() / len(sixers_data)
sixers_oreb = sixers_data.OREB.sum() / len(sixers_data)
sixers_dreb = sixers_data.DREB.sum() / len(sixers_data)
sixers_ast = sixers_data.AST.sum() / len(sixers_data)
sixers_stl = sixers_data.STL.sum() / len(sixers_data)
sixers_tov = sixers_data.TOV.sum() / len(sixers_data)
sixers_blk = sixers_data.BLK.sum() / len(sixers_data)
sixers_pf = sixers_data.PF.sum() / len(sixers_data)
sixers_pts = sixers_data.PTS.sum() / len(sixers_data)

